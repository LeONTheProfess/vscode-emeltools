{
    "name": "vscode-emel",
    "displayName": "EME-L",
    "description": "",
    "version": "0.1.0",
    "publisher": "LeONTheProfess",
    "repository": {
        "type": "git",
        "url": "https://github.com/LeONTheProfess/vscode-emeltools.git"
    },
    "engines": {
        "vscode": "^1.61.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "emel",
            "aliases": ["emel", "emel"],
            "extensions": [".emel"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "emel",
            "scopeName": "source.emel",
            "path": "./syntaxes/emel.tmLanguage.json"
        }],
        "snippets": [
            {
                "language": "emel",
                "path": "./snippets/emel.snippets.json"
            }
        ]
    }
}